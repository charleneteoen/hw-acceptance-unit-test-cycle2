<h2>Movies with Same Director as <%= @movie.title %></h2>

<h1>Movies with Same Director</h1>
<% @movies_with_same_director.each do |movie| %>
  <p><%= movie.title %></p>
<ul>
  <% @movies_with_same_director.each do |movie| %>
    <li><%= link_to movie.title, edit_movie_path(movie) %></li>
  <% end %>
</ul>